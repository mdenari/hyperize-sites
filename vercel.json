{
 "rewrites": [
      {
        "source": "/apresentacoes/:path*",
        "has": [
          {
            "type": "host",
            "value": "docs.hyperize.com.br"
          }
        ],
        "destination": "/apresentacoes/:path*"
      },
      {
        "source": "/",
        "has": [
          {
            "type": "host",
            "value": "docs.hyperize.com.br"
          }
        ],
        "destination": "/index.html"
      },
      {
        "source": "/",
        "has": [
          {
            "type": "host",
            "value": "show.hyperize.com.br"
          }
        ],
        "destination": "/apresentacao_comercial_btp.html"
      },
      {
        "source": "/:path*",
        "destination": "/:path*"
      }
   ]
}